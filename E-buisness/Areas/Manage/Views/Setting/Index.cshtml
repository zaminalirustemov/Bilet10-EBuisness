@model List<Settings>
@{
    int count = 0;
}

<div class="container">
    <h2>Settings/Table</h2>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">№</th>
                <th scope="col">Key</th>
                <th scope="col">Value</th>
            </tr>
        </thead>
        <tbody>
            @foreach (Settings setting in Model)
            {
                count++;
                <tr>
                    <th scope="row">@count</th>
                    <td>@setting.Key</td>
                    <td>@setting.Value</td>
                    <td>
                        <a asp-action="update" asp-controller="setting" asp-route-id="@setting.Id" class="btn btn-outline-primary">Update</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>